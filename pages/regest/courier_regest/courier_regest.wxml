<!--pages/regest/regest.wxml-->


<view class='my_container'>
    <van-cell-group>
    <van-field
        value=""
        required
        label="公司名称"
        left-icon="home-o"
        placeholder="所属快递公司名称"
        bind:change="onChange"
        data-obj="courier"
        data-item="company"
      />
      <van-field
        value=""
        required
        label="姓名"
        left-icon="contact"
        placeholder="请输入姓名"
        bind:change="onChange"
        data-obj="courier"
        data-item="name"
      />
      <van-field
        value=""
        required
        label="手机号"
        left-icon="phone-o"
        placeholder="国家区号+手机号"
        bind:change="onChange"
        data-obj="courier"
        data-item="phone"
      />
      <van-field
        value=""
        required
        label="邮件"
        left-icon="comment-circle-o"
        placeholder="电子邮件"
        bind:change="onChange"
        data-obj="courier"
        data-item="email"
      />
      <van-field
      value=""
      required
      disabled
      clearable
      label="上传图片"
      left-icon="photo-o"
      icon="add-o"
      placeholder=""
      bind:click-icon="uploadImg"
    />
    <view class="my_center_container">
      <image mode='aspectFit' src="{{imgSrc}}" wx:if="{{imgSrc}}"></image>
    </view>
  </van-cell-group>
  </view>
<view class='my_center_container'>
  <van-button type="danger"  size="large" bindtap='commit'>提交</van-button>
</view>